<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="users">

	<!-- 쿼리문 작성 -->

	<select id="selectUsersList" resultType="com.javaex.vo.UsersVo">
		<![CDATA[
			select * from users
		]]>
	</select>

	<select id="selectUserNickNameList"
		resultType="com.javaex.vo.UsersVo">
		<![CDATA[
			
			select users.nickname
 from wordbook,directory,users
 where users.userno=directory.userno
 and directory.directoryno=wordbook.wordbookno
 and users.id = 'seo'			
		]]>
	</select>

</mapper>
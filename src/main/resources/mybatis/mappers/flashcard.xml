<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="flashcard">

	<!-- 쿼리문 작성 -->

	<select id="selectFlashcardWordList" resultType="com.javaex.vo.WordbookVo">
		<![CDATA[
			select seyawordname
			from seyaclass, seyaword_eng
			where seyaclass.seyawordno = seyaword_eng.seyawordno
			and seyaclass.seyawordbookno = 
				(select seyawordbookno
				from seyawordbook
				where seyawordbookname = '내 단어장1')
		]]>
	</select>
